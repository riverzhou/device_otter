/dev/block/mmcblk0p10           /data           ext4            noatime,nodiratime,nosuid,nodev,discard,nomblk_io_submit,noauto_da_alloc,barrier=0,errors=panic wait
/dev/block/mmcblk0p8            /dropbox        ext4            noatime,nodiratime,nosuid,nodev,discard,nomblk_io_submit,noauto_da_alloc,barrier=0,errors=panic wait

